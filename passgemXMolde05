import PySimpleGUI as sg
from PIL import Image , ImageTk
import os 
import io

#lista das passagem biblicas 
passagemBi = ['Gênesis','Salmo', 'Proverbio', 'Icorintios', 'Apocalipse']
# para ver a referência Biblicas
def Referencia(livroX, capituloX,versiculox): # argumento para dentro da função
        if livroX == 'Proverbio':
            if capituloX == '1' :
                if versiculox == '1':
                    print('Referência Biblica do versículo 1Rs 4.32')
                    print('*'*7)
        
            
                elif versiculox == '7':        
                    print('Referência Biblica do versículo Pv 9.10')
                    print('*'*7)
        
        
                elif versiculox == '20':
                    print('Referência Biblica do versículo Pv 8.1-3') 
                    print('*'*7)

              
              
        elif livroX == 'Salmo':
            if capituloX == '35' and versiculox == '19':
                print('Referência Biblica do versículo Sl 69.4 , Jo 15.25')
                print('*'*7)
            elif capituloX == '117' and versiculox == '1':
                print('Referência Biblica do versículo Rm 15.11')
                print('*'*7)
        elif livroX == 'Icorintios' and capituloX == '13' and versiculox == '2':
            print('Referência Biblica do versículo Mt 17.20 ; 21.21 ; Mc 11.23')
            print('*'*7)

##############################
# ver livros capitulos é versículos
def Imprimindo(livroX,capituloX,versiculox,cap):

    verso = int(versiculox)

    if versiculox == '0':
    
        print(cap.values()) 
    if verso > len(cap): # usei essa condição para evitar um erro

        print('versiculo inexistente')
    else:
        if versiculox != '0':
            sg.popup_scrolled(cap[versiculox],title='versículo')
            print(cap[versiculox])
        
            #para ver a quantidade de versiculos acho que posso retorna para quantidadeV
                
            #versiculo = int(versiculox)
            quantidadeverso = len(cap) #-1
            print(f'{livroX} {capituloX} tem {quantidadeverso} versículos')   
            print('*'*7)   

     
def VersiculoContagem(capituloX,versiculox,livroX,cap):
    quantidadeV = len(cap) # quantidade total de versículo
   
    # usei essa lista para verificar a quantidade de versiculos para essa função

    listaversiculo = []             # essa lista vai ser criada por conta da quantidade de versiculos dada nas codições abaixo
    
    versiculoInt = int(versiculox)  # convertendo str para inteiro

    for i in range(quantidadeV+1):  # quantidade de versiculos com um a mais +1 veio das condições mais abaixo para cada capitulo dos livros 
                    
        listaversiculo.append(i)    # criando uma lista de numeros inteiro
        #print(i) # mostra todos os versículos

    else:   
        if versiculoInt in listaversiculo: # vendo se o versiculo esta no Salmo pois este esta vendo se esta dentro da lista de versiculos

            if livroX == 'Salmo':                       
                print(Salmo(livroX,capituloX,versiculox)) # o print servio para abri a função feita por mim é printa capitulo e versículo
            if livroX == 'Proverbio':
                print(Proverbio(livroX,capituloX,versiculox))
            if livroX == 'Icorintios':
                print(Icorintios(livroX,capituloX,versiculox))
            if livroX == 'Gênesis':
                print(Genesis(livroX,capituloX,versiculox))
        else:
            print('verso não existe')




def Genesis(livroX,capituloX,versiculox):
    if livroX == 'Gênesis':
        if capituloX == '1':
            

            cap = {
            #'0' : 'fazer estudo ainda', 

            '1':'1 No princípio criou Deus os céus e a terra.',
            '2':'2 A terra era sem forma e vazia; e havia trevas sobre a face do abismo, mas o Espírito de Deus pairava sobre a face das águas.',
            '3':'3 Disse Deus: haja luz. E houve luz.',
            '4':'4 Viu Deus que a luz era boa; e fez separação entre a luz e as trevas.',
            '5':'5 E Deus chamou à luz dia, e às trevas noite. E foi a tarde e a manhã, o dia primeiro.',
            '6':'6 E disse Deus: haja um firmamento no meio das águas, e haja separação entre águas e águas.',
            '7':'7 Fez, pois, Deus o firmamento, e separou as águas que estavam debaixo do firmamento das que estavam por cima do firmamento. E assim foi.',
            '8':'8 Chamou Deus ao firmamento céu. E foi a tarde e a manhã, o dia segundo.',
            '9':'9 E disse Deus: Ajuntem-se num só lugar as águas que estão debaixo do céu, e apareça o elemento seco. E assim foi.',
            '10':'10 Chamou Deus ao elemento seco terra, e ao ajuntamento das águas mares. E viu Deus que isso era bom.',
            '11':'11 E disse Deus: Produza a terra relva, ervas que dêem semente, e árvores frutíferas que, segundo as suas espécies, dêem fruto que tenha em si a sua semente, sobre a terra. E assim foi.',
            '12':'12 A terra, pois, produziu relva, ervas que davam semente segundo as suas espécies, e árvores que davam fruto que tinha em si a sua semente, segundo as suas espécies. E viu Deus que isso era bom.',
            '13':'13 E foi a tarde e a manhã, o dia terceiro.',
            '14':'14 E disse Deus: haja luminares no firmamento do céu, para fazerem separação entre o dia e a noite; sejam eles para sinais e para estações, e para dias e anos;',
            '15':'15 e sirvam de luminares no firmamento do céu, para alumiar a terra. E assim foi.',
            '16':'16 Deus, pois, fez os dois grandes luminares: o luminar maior para governar o dia, e o luminar menor para governar a noite; fez também as estrelas.',
            '17':'17 E Deus os pôs no firmamento do céu para alumiar a terra,',
            '18':'18 para governar o dia e a noite, e para fazer separação entre a luz e as trevas. E viu Deus que isso era bom.',
            '19':'19 E foi a tarde e a manhã, o dia quarto.',
            '20':'20 E disse Deus: Produzam as águas cardumes de seres viventes; e voem as aves acima da terra no firmamento do céu.',
            '21':'21 Criou, pois, Deus os monstros marinhos, e todos os seres viventes que se arrastavam, os quais as águas produziram abundantemente segundo as suas espécies; e toda ave que voa, segundo a sua espécie. E viu Deus que isso era bom.',
            '22':'22 Então Deus os abençoou, dizendo: Frutificai e multiplicai-vos, e enchei as águas dos mares; e multipliquem-se as aves sobre a terra.',
            '23':'23 E foi a tarde e a manhã, o dia quinto.',
            '24':'24 E disse Deus: Produza a terra seres viventes segundo as suas espécies: animais domésticos, répteis, e animais selvagens segundo as suas espécies. E assim foi.',
            '25':'25 Deus, pois, fez os animais selvagens segundo as suas espécies, e os animais domésticos segundo as suas espécies, e todos os répteis da terra segundo as suas espécies. E viu Deus que isso era bom.',
            '26':'26 E disse Deus: Façamos o homem à nossa imagem, conforme a nossa semelhança; domine ele sobre os peixes do mar, sobre as aves do céu, sobre os animais domésticos, e sobre toda a terra, e sobre todo réptil que se arrasta sobre a terra.',
            '27':'27 Criou, pois, Deus o homem à sua imagem; à imagem de Deus o criou; homem e mulher os criou.',
            '28':'28 Então Deus os abençoou e lhes disse: Frutificai e multiplicai-vos; enchei a terra e sujeitai-a; dominai sobre os peixes do mar, sobre as aves do céu e sobre todos os animais que se arrastam sobre a terra.',
            '29':'29 Disse-lhes mais: Eis que vos tenho dado todas as ervas que produzem semente, as quais se acham sobre a face de toda a terra, bem como todas as árvores em que há fruto que dê semente; ser-vos-ão para mantimento.',
            '30':'30 E a todos os animais da terra, a todas as aves do céu e a todo ser vivente que se arrasta sobre a terra, tenho dado todas as ervas verdes como mantimento. E assim foi.',
            '31':'31 E viu Deus tudo quanto fizera, e eis que era muito bom. E foi a tarde e a manhã, o dia sexto.',
            }

##############################################################################################################
