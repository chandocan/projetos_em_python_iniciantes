def Icorintios(livroX,capituloX,versiculox):
    if livroX == 'Icorintios':
        if capituloX == '13':
            

            cap = {
            #'0' : 'fazer estudo ainda', 

            '1' : '1 Ainda que eu falasse as linguás dos homens e dos anjos, e não tivesse amor, seria como o metal que soa ou como o címbalo que retine.',

            '2' : '2 E ainda que tivesse o dom de profecia, e conhecesse todos os mistérios e toda a ciência, e ainda que tivesse toda fé, de maneira tal que transportasse os montes, e não tivesse amor, nada seria.',

            '3' : '3 E ainda que distribuísse todos os meus bens para sustento dos pobres, e ainda que entregasse o meu corpo para ser queimado, e não tivesse amor, nada disso me aproveitaria.',

            '4' : '4 O amor  sofredor, é benigno; o amor não é invejoso; o amor não se vangloria, não se ensoberbece,',

            '5' : '5 não se porta inconvenientemente, não busca os seus próprios interesses, não se irrita, não suspeita mal;',

            '6' : '6 não se regozija com a injustiça, mas se regozija com a verdade;',

            '7' : '7 tudo sofre, tudo cré, tudo espera, tudo suporta.',

            '8' : '8 O amor jamais acaba; mas havendo profecias, serão aniquiladas; havendo linguás, cessarão; havendo ciência, desaparecera;',

            '9' : '9 porque, em parte conhecemos, e em parte profetizamos;',

            '10' : '10 mas, quando vier o que é perfeito, então o que é em parte será aniquilado.',

            '11' : '11 Quando eu era menino, pensava como menino; mas, logo que cheguei a ser homem, acabei com as coisas de menino.',

            '12' : '12 Porque agora vemos como por espelho, em enigma, mas então veremos face a face; agora conheço em parte, mas então conhecerei plenamente, como também sou plenamente conhecido.',

            '13' : '13 Agora, pois, permanecem a fé, a esperança, o amor, estes três; mas o maior destes é o amor.'
            }
            
            Imprimindo(livroX,capituloX,versiculox,cap)
            VersiculoContagem(livroX,capituloX,versiculox,cap)




def Apocalipse():
    print('Apocalipse Teste rico é morto!')


# abrindo historias em quadrinhos
def AbrindoHistorias():
    
    #import PySimpleGUI as sg
    #import os
    #from PIL import Image, ImageTk
    #import io


    def janela_Historias():
        sg.theme('Reddit')
        layout = [
            [sg.Text('Historias Biblicas nas Pastas', size= (23,1), font='Lucida')],
            #[sg.Listbox(values=['Egito Antigo','Israel','Monte Sinai','Rio Nilo','Monte Ararat','Rio Jordão','Jardim do getsêmani','Segundo Templo','Babilônia','Mar da Galileia','ArvoresGethsêmane' ], select_mode='extended' ,size=(30,6), key='map')],
            [sg.Button('voltar',key= 'voltar') ,sg.Button('Abrie', key= 'Abrie_hist'),sg.Button('ouvir', key= 'ouvir')]
        ]

        #return sg.Window('Historias', layout=layout,finalize=True)
        janela6 =  sg.Window('Historias', layout=layout, finalize = True) # aqui esta a variavel janela3 recendo a sg.Windows aquesta a barra de titulo
        #return janela6
 
        while True:            
            #estraindo dados
         
            #event, values = janela6.Read()
            event,values  = janela6.read()
         
            if event == 'voltar':
                janela6.close()
            #self.windows,self.event, self.values = sg.read_all_windows() 
            #event, values = sg.read_all_windows() 
            if event is None:
                break
            if event == '\r':
                elem = janela6.FindElementWithFocus()
                if elem is not None:
                    elem.Click()

            elif event == 'voltar':

                print('ola 33 teste de voltar')

            elif event == 'ouvir':

                print('para ouvir as historias em quadrinhos')   

            elif event == 'Abrie_hist':

                print('teste2 de abrie')

                folder = sg.popup_get_folder('Historias Biblicas',initial_folder=True, default_path='IMAGEM_D_MAPAS/historiasBiblicas/')
                  # PIL supported image types
                img_types = (".png", ".jpg", "jpeg", ".tiff", ".bmp")

                # get list of files in folder
                flist0 = os.listdir(folder)

                # create sub list of image files (no sub folders, no wrong file types)
                fnames = [f for f in flist0 if os.path.isfile(
                    os.path.join(folder, f)) and f.lower().endswith(img_types)]

                num_files = len(fnames)                # number of iamges found
                if num_files == 0:
                    sg.popup('No files in folder')
                    raise SystemExit()

                del flist0                             # no longer needed

                # ------------------------------------------------------------------------------
                # use PIL to read data of one image
                # ------------------------------------------------------------------------------


                def get_img_data(f, maxsize=(900, 750), first=False): # 1200 , 850
                    """Generate image data using PIL
                    """
                    img = Image.open(f)
                    img.thumbnail(maxsize)
                    if first:                     # tkinter is inactive the first time
                        bio = io.BytesIO()
                        img.save(bio, format="PNG")
                        del img
                        return bio.getvalue()
                    return ImageTk.PhotoImage(img)
                # ------------------------------------------------------------------------------


                # make these 2 elements outside the layout as we want to "update" them later
                # initialize to the first file in the list
                filename = os.path.join(folder, fnames[0])  # name of first file in list
                image_elem = sg.Image(data=get_img_data(filename, first=True))
                filename_display_elem = sg.Text(filename, size=(80, 3))
                file_num_display_elem = sg.Text('File 1 of {}'.format(num_files), size=(15, 1))

                # define layout, show and read the form
                col = [[filename_display_elem],
                    [image_elem]]

                col_files = [[sg.Listbox(values=fnames, change_submits=True, size=(30, 30), key='listbox')], # temos a listas das imagens aqui
                            [sg.Button('Next', size=(8, 2)), sg.Button('Prev', size=(8, 2)), file_num_display_elem]]

                layout = [[sg.Column(col_files), sg.Column(col)]]

                window = sg.Window('Image Browser', layout, return_keyboard_events=True,
                                location=(0, 0), use_default_focus=False)

                # loop reading the user input and displaying image, filename
                i = 0
                while True:
                    # read the form
                    event, values = window.read()
                    print(event, values)
                    # perform button and keyboard operations
                    if event == sg.WIN_CLOSED:
                        break
                    elif event in ('Next', 'MouseWheel:Down', 'Down:40', 'Next:34'):
                        i += 1
                        if i >= num_files:
                            i -= num_files
                        filename = os.path.join(folder, fnames[i])
                    elif event in ('Prev', 'MouseWheel:Up', 'Up:38', 'Prior:33'):
                        i -= 1
                        if i < 0:
                            i = num_files + i
                        filename = os.path.join(folder, fnames[i])
                    elif event == 'listbox':            # something from the listbox
                        f = values["listbox"][0]            # selected filename
                        filename = os.path.join(folder, f)  # read this file
                        i = fnames.index(f)                 # update running index
                    else:
                        filename = os.path.join(folder, fnames[i])

                    # update window with new image
                    image_elem.update(data=get_img_data(filename, first=True))
                    # update window with filename
                    filename_display_elem.update(filename)
                    # update page display
                    file_num_display_elem.update('File {} of {}'.format(i+1, num_files))

                window.close()
                AbrindoHistorias()
                
    janela_Historias() 

